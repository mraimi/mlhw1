function params = hw1_train1a(X,Y)
    load('mnist.mat');
    params = zeros(10,1);
    for q=1:60000
            params((Y(q)+1),1) = params((Y(q)+1),1) + 1;
    end
    params = params/60000.0;
 
    mu = zeros(10,784);
    for q=1:60000
            mu((Y(q)+1),:) =  mu((Y(q)+1),:) + X(q,:)/params((Y(q)+1),1);
    end
    mu = mu/(60000.0);
    
    diff = zeros(60000,784);
    sigma = zeros(10,784);
    
    
    for q=1:60000
        diff(q,:) = X(q,:)- mu((Y(q)+1), :);
    end
    
    sigma((Y(q)+1),1) = sigma((Y(q)+1),1) + diff()
    
    
end